<div class="author">
  <h1 class="author-heading">Search Results</h1>
</div>

<section class="articles">
  <ul class="article-ul">
    <% posts.forEach(post => { %>
      <li>
        <a href="/posts/<%= post._id %>">
          <span><%= post.title %></span>
          <span class="article-list-date"><%= post.createdAt.toDateString() %></span>
        </a>
      </li>
    <% }) %>
  </ul>
</section>
